FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.0;

boundaryField

{
	top{
type            fixedFluxPressure;
value           uniform 0;
	}

	bottom{
type            fixedFluxPressure;
value           uniform 0;
	}
	
	right{
type            fixedFluxPressure;
value           uniform 0;
	}
	
	left{
type            fixedFluxPressure;
value           uniform 0;
	}
	
	frontAndBack{
type            fixedFluxPressure;
value           uniform 0;
	}
}

